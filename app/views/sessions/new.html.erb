<h1>ログイン</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

  <div>
    <%= f.label :email, 'Email' %>
    <%= f.email_field :email %>
  </div>

  <div>
    <%= f.label :password, 'Password' %>
    <%= f.password_field :password %>
  </div>

  <%= f.submit 'Log in' %>
<% end %>

    <p>新規の方はこちら <%= link_to '登録', signup_path %></p>